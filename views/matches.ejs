<%- include('layouts/main.ejs'); %>
<%- include('partials/header.ejs'); %>

<section class="matches">
    <h1>Dit zijn jouw matches</h1>

<ul class="lijst">
    <% likedDogs.forEach(function(likedDogs) { %>
        <li id="match1">
            <img class="fotohond" src="<%= likedDogs.img %>" alt="">
            <h2><%= likedDogs.name %></h2>
            <p><%= likedDogs.description %></p>
            <form id="delete" action="/delete" method="POST">
                <input name="liked" type="hidden" value="DELETE">
                <input name="id" type="hidden" value="<%= userID %>">
                <button type="submit" id="verwijder">Verwijder match</button>  
            </form>         
        </li>
    <% }); %>    
</ul>
</section>



<script src="/static/js/script.js"></script>

